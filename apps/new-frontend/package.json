{
	"name": "new-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"setup": "bun install",
		"dev": "cp .env .env.local && bunx next dev --turbopack -p 3001",
		"build-local": "cp .env .env.local && bunx next build",
		"build-dev": "cp .env.development .env.local && bunx --bun next build",
		"build-prod": "cp .env.production .env.local && bunx --bun next build",
		"start": "next start -p 3001",
		"format": "bunx biome format --write .",
		"lint": "bunx biome lint --write ./src"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@hookform/resolvers": "^5.2.1",
		"@shared/tooling": "workspace:*",
		"@shared/ui": "workspace:*",
		"axios": "^1.11.0",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"jwt-decode": "^4.0.0",
		"next": "15.5.2",
		"react": "19.1.1",
		"react-countup": "^6.5.3",
		"react-dom": "19.1.1",
		"react-hook-form": "^7.62.0",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"typescript": "^5",
		"@types/node": "^24.3.0",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@tailwindcss/postcss": "^4",
		"tailwindcss": "^4"
	}
}

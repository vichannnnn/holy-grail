{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"setup": "bun install",
		"dev": "bunx next dev --turbopack",
		"build": "bunx next build",
		"build-dev": "cp .env.development .env.local && bunx next build",
		"build-prod": "cp .env.production .env.local && bunx next build",
